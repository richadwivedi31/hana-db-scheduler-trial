{
  "name": "scheduler_trial",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.19",
    "@sap/hana-client": "^2.15.22",
    "@sap/hdbext": "^7.7.2",
    "express": "^4",
    "hdb": "^0.19.0",
    "sap-hdbext-promisfied": "^2.202008.1"
  },
  "scripts": {
    "start": "cds run"
  },
  "cds": {
    "build": {
      "tasks": [
        {
          "for": "hana",
          "dest": "../db"
        },
        {
          "for": "node-cf"
        }
      ]
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "devDependencies": {
    "@sap/hdi-deploy": "4.5.3"
  }
}
