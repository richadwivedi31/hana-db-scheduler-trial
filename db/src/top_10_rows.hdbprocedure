PROCEDURE "top_10_rows"() 
  LANGUAGE SQLSCRIPT 
  SQL SECURITY INVOKER 
  --DEFAULT SCHEMA <default_schema_name>
  READS SQL DATA AS 
BEGIN
  SELECT TOP 10 "MANUFACTURER",
    "MODEL_NAME",
    "CATEGORY",
    "SCREEN_SIZE",
    "SCREEN",
    "CPU",
    "RAM",
    "STORAGE",
    "GPU",
    "OPERATING_SYSTEM",
    "OPERATING_SYSTEM_VERSION",
    "WEIGHT",
    "PRICE_IN_EUROS"
  FROM "PRODUCT_DETAILS_PRODUCTS";
END